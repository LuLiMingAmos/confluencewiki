                dialog.searcher.focus();
            }, "dialog-back-link")
            .addCancel("\u53D6\u6D88", function () {
                // todo: does it need return value? can we just pass the reference to onCancel?
                return options.onCancel();
            });

        dialog.gotoPage(2);
        dialog.addHeader("\u65E0\u5B8F\u7684\u5143\u6570\u636E");

