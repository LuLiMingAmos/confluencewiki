        dialog.addHeader(options.title);

        // menu on the left, setup category panels
        dialog.addPanel("\u5168\u90E8", categorySummaryHTMLElements.all,
            "all", "category-button-all");

        for (i = 0, ii = options.categories.length; i < ii; i++) {
